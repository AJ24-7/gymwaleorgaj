{
  "version": 2,
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.{js,css,png,jpg,jpeg,gif,svg,ico}",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "^/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "^/admin/(.*)",
      "dest": "/admin/$1"
    },
    {
      "src": "^/gymadmin/(.*)",
      "dest": "/gymadmin/$1"
    },
    {
      "src": "^/Trainer/(.*)",
      "dest": "/Trainer/$1"
    },
    {
      "src": "^/modules/(.*)",
      "dest": "/modules/$1"
    },
    {
      "src": "^/data/(.*)",
      "dest": "/data/$1"
    },
    {
      "src": "^/(.*\\.(css|js|html|png|jpg|jpeg|gif|svg|ico))$",
      "dest": "/$1"
    },
    {
      "src": "^/$",
      "dest": "/index.html"
    },
    {
      "src": "^/(.*)",
      "dest": "/$1"
    }
  ],
  "github": {
    "silent": true
  }
}
